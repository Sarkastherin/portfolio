---
import BaseLayout from "../layouts/BaseLayout.astro";
import HeroSection from "../components/HeroSection.astro";
import Services from "../components/Services.astro";
import StartProjects from "../components/StartProjects.astro";
import AboutMe from "../components/AboutMe.astro";
import Testimonies from "../components/Testimonies.astro";
import CTA from "../components/CTA.astro";

const pageTitle = "Katherine Dev - Desarrolladora Web Freelance";
const description = "Desarrolladora web freelance especializada en React, TypeScript y Astro. Creo aplicaciones modernas, automatizo procesos y desarrollo soluciones digitales personalizadas para empresas.";
const keywords = [
  "desarrolladora web freelance",
  "react developer",
  "typescript",
  "astro",
  "automatización procesos",
  "aplicaciones web",
  "desarrollo frontend",
  "soluciones digitales",
  "argentina",
  "katherine dev"
];
---

<BaseLayout 
  pageTitle={pageTitle}
  description={description}
  keywords={keywords}
  type="website"
>
  <HeroSection />
  <Services />
  <StartProjects />
  <AboutMe />
  <Testimonies />
  <CTA
    title="¿Tienes una idea o proceso que querés digitalizar?"
    description="Estoy acá para ayudarte a hacerlo realidad."
    href="/contactme"
    buttonTitle="Contáctame"
  />
</BaseLayout>
